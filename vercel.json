{
    "version": 2,
    "framework": null,
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.6.1" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "public/favicon.ico"
        },
        {
            "src": "^/(.*).(png|jpg|jpeg)",
            "dest": "public/images/$1.$2"
        },
        {
            "src": "^/(.*).(css)",
            "dest": "public/css/$1.css"
        },
        {
            "src": "^/(.*).(js)",
            "dest": "public/js/$1.js"
        },
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://toros-vacas-helkaraxes-projects.vercel.app",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "LOG_CHANNEL": "stderr",

        "APP_NAME":"Toros y Vacas",
        "APP_TIMEZONE":"UTC",

        "APP_LOCALE":"es",
        "APP_FALLBACK_LOCALE":"es",
        "APP_FAKER_LOCALE":"es_ES",

        "DB_CONNECTION":"pgsql",

        "CACHE_STORE":"database",

        "GAME_TIME":"300"
    }
}
